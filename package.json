{
	"name": "django-import-libraries",
	"displayName": "Django Libraries",
	"description": "List of Libraries to be imported in Django",
	"publisher": "DhavalVira",
	"icon": "django.png",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.50.0"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/dhavalveera/vscode-extension-django-libraries"
	},
	"keywords": [
		"python",
		"django",
		"web"
	],
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	
	"contributes": {
		"languages": [
			{
				"id": "python",
				"aliases": [
					"Python"
				],

				"extensions": [
					".py",
					".pyi",
					".pyc",
					".pyd",
					".pyo",
					".pyw",
					".pyz"
				]
			}
		],
		"snippets": [
			{
				"language": "python",
				"path": "./snippets/python.json"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.50.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
